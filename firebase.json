
{
  "hosting": {
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "headers": [ {
      "source" : "**/*",
      "headers" : [ {
        "key" : "x-frame-options",
        "value" : "DENY"
      } ]
    } ],
    "cleanUrls": true,
    "trailingSlash": false
  }
}