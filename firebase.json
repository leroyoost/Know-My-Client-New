{
  "hosting": {
    "public": "dist",
    "rewrites": [
      {
        "source": "/api/bankSubmit",
        "function": "bankSubmit"
      },
      {
        "source": "/api/idVerify",
        "function": "idVerify"
      },
      {
        "source": "/api/bankRequest",
        "function": "bankRequest"
      },
      {
        "source": "/api/idTrace",
        "function": "idTrace"
      }
    ],
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "headers": [
      {
        "source": "**",
        "headers": [
          {
            "key": "x-frame-options",
            "value": "DENY"
          }
        ]
      }
    ],
    "cleanUrls": true,
    "trailingSlash": false
  }
}