{
  "hosting": {
    "public": "dist",
    "rewrites": [
      {
        "source": "/api/avsSubmit",
        "function": "avsSubmit"
      },
      {
        "source": "/api/bulkAvsSubmit",
        "function": "bulkAvsSubmit"
      },
      {
        "source": "/api/avsClientWrite",
        "function": "avsClientWrite"
      },
      {
        "source": "/api/idVerify",
        "function": "idVerify"
      },
      {
        "source": "/api/avsStatus",
        "function": "avsStatus"
      },
      {
        "source": "/api/idTrace",
        "function": "idTrace"
      }
    ],
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "headers": [
      {
        "source": "**",
        "headers": [
          {
            "key": "x-frame-options",
            "value": "DENY"
          }
        ]
      }
    ],
    "cleanUrls": true,
    "trailingSlash": false
  }
}