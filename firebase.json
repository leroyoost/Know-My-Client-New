{
  "hosting": {
    "public": "dist",
    "rewrites": [ {
      "source": "https://secure.knowmyclient.com/api/idVerify", "function": "idVerify"
    },
    {
      "source": "https://secure.knowmyclient.com/api/bankVerify", "function": "bankVerify"
    } ],
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "headers": [ {
      "source" : "**",
      "headers" : [ {
        "key" : "x-frame-options",
        "value" : "DENY"
      } ]
    } ],
    "cleanUrls": true,
    "trailingSlash": false
  }
}